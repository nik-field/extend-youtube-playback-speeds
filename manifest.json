{
  "manifest_version": 3,
  "name": "Youtube Speed Options Extender",
  "version": "0.1.0",
  "description": "Extends the available playback rate options on YouTube",
  "permissions": ["activeTab"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "run_at": "document_end",
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [{
    "resources": ["inject.js"],
    "matches": ["<all_urls>"]
  }]
}
